{
	"name": "shotstack-forms",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"test": "jest --verbose --silent",
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"cypress": "cypress open",
		"package": "PACKAGE=package vite build && cp ./package/Shotstack.iife.js ./package/forms.min.js && cp ./package/style.css ./package/forms.min.css",
		"package:win": "set PACKAGE=package && vite build",
    "deploy": "aws s3 sync package/ s3://js.shotstack.io/forms/1.0/ --exclude 'README.md' --exclude 'Shotstack.*' --exclude 'style.css'"
	},
	"devDependencies": {
		"@babel/core": "^7.19.1",
		"@babel/preset-env": "^7.19.1",
		"@babel/preset-typescript": "^7.18.6",
		"@jest/globals": "^29.3.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/vite-plugin-svelte": "^1.1.0",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/svelte": "^3.2.2",
		"@tsconfig/svelte": "^3.0.0",
		"@types/jest": "^29.2.2",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.11",
		"babel-jest": "^29.0.3",
		"cypress": "^10.8.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"husky": "^8.0.1",
		"jest": "^29.0.3",
		"jest-environment-jsdom": "^29.0.3",
		"postcss": "^8.4.16",
		"postcss-prefixwrap": "1.35.1",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-jester": "^2.3.2",
		"svelte-preprocess": "^4.10.7",
		"tailwindcss": "^3.1.8",
		"ts-jest": "^29.0.2",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"vite": "^3.1.0"
	},
	"type": "module"
}
